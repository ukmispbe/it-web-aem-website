<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="sling:OsgiConfig"
          resource.resolver.allowDirect="{Boolean}true"
          resource.resolver.manglenamespaces="{Boolean}true"
          resource.resolver.map.location="/etc/map.publish"
          resource.resolver.mapping="[/-/,/content/waters/:/nextgen/,/content/orders/-/]"
          resource.resolver.searchpath="[/apps,/libs]"
          resource.resolver.virtual="[/:/]"
          resource.resolver.required.providernames="[JCR]"
          resource.resolver.enable.vanitypath="{Boolean}true"
          resource.resolver.optimize.alias.resolution="{Boolean}true"
          resource.resolver.default.vanity.redirect.status="302"
          resource.resolver.vanitypath.bloomfilter.maxBytes="{Long}1024000"
          resource.resolver.vanity.precedence="{Boolean}true"
          resource.resolver.providerhandling.paranoid="{Boolean}false"
          resource.resolver.log.closing="{Boolean}false"
          resource.resolver.log.unclosed="{Boolean}false"
          resource.resolver.vanitypath.maxEntries="{Long}-1"
          resource.resolver.vanitypath.maxEntries.startup="{Boolean}true"
          resource.resolver.vanitypath.whitelist="[/apps/,/libs/,/content/]"
          resource.resolver.vanitypath.blacklist="[/content/usergenerated]"/>