<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Page"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[cq.common.wcm,core.wcm.components.page.v2.editor,cq.wcm.msm.properties,granite.contexthub.configuration,cq.siteadmin.admin.properties]"
          helpPath="https://www.adobe.com/go/aem_cmp_page_v2"
          mode="edit">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <thumbnail
                       jcr:primaryType="nt:unstructured"
                        path="waters/components/structure/page/thumbnail/content"
                        sling:resourceType="granite/ui/components/foundation/include"
                        sling:orderBefore="cloudservices"/>
                    <content
                            cq:hideOnEdit="{Boolean}false"
                            cq:showOnCreate="{Boolean}false"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Open Graph"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            sling:orderBefore="socialmedia"
                            margin="{Boolean}false"
                            maximized="{Boolean}false">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedColumns
                                    cq:hideOnEdit="{Boolean}false"
                                    cq:showOnCreate="{Boolean}false"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            cq:hideOnEdit="{Boolean}false"
                                            cq:showOnCreate="{Boolean}false"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            margin="{Boolean}false"
                                            maximized="{Boolean}false">
                                        <items jcr:primaryType="nt:unstructured">
                                            <canonicalUrl
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Canonical URL"
                                                    name="./canonicalUrl"
                                                    required="{Boolean}false"
                                                    rootPath="/">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="canonicalUrl"/>
                                            </canonicalUrl>
                                            <seoTitle
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="SEO Title"
                                                    name="./seoTitle"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="seoTitle"/>
                                            </seoTitle>
                                            <metaDescription
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Description"
                                                    name="./metaDescription"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false"
                                                    resize="none"
                                                    rows="5">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="metaDescription"/>
                                            </metaDescription>
                                            <metaSchema
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="MetaSchema"
                                                    name="./metaSchema"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false"
                                                    resize="none">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="metaSchema"/>
                                            </metaSchema>
                                            <noIndex
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="No Index"
                                                    ignoreData="{Boolean}false"
                                                    name="./noIndex"
                                                    offText="No"
                                                    onText="Yes"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="noIndex"/>
                                            </noIndex>
                                            <noFollow
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="No Follow"
                                                    ignoreData="{Boolean}false"
                                                    name="./noFollow"
                                                    offText="No"
                                                    onText="Yes"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="noFollow"/>
                                            </noFollow>
                                            <ogType
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Open Graph Type"
                                                    multiple="{Boolean}false"
                                                    name="./ogType"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option0
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="None"
                                                            value="none"/>
                                                    <option1
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Article"
                                                            value="article"/>
                                                    <option2
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Book"
                                                            value="book"/>
                                                    <option3
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Profile"
                                                            value="profile"/>
                                                    <option4
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Website"
                                                            value="website"/>
                                                    <option5
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Movie"
                                                            value="video.movie"/>
                                                    <option6
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Episode"
                                                            value="video.episode"/>
                                                    <option7
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="TV Show"
                                                            value="video.tv_show"/>
                                                    <option8
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Video"
                                                            value="video.other"/>
                                                    <option9
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Song"
                                                            value="music.song"/>
                                                    <option10
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Album"
                                                            value="music.album"/>
                                                    <option11
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Playlist"
                                                            value="music.playlist"/>
                                                    <option12
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Radio Station"
                                                            value="music.radio_station"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="ogType"/>
                                            </ogType>
                                            <ogImage
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Open Graph Image"
                                                    filter="hierarchyNotFile"
                                                    forceSelection="{Boolean}false"
                                                    name="./ogImage"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam/waters">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="ogImage"/>
                                            </ogImage>
                                            <facebookAppId
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Facebook App ID"
                                                    name="./facebookAppId"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="facebookAppId"/>
                                            </facebookAppId>
                                            <twitterPublisherHandle
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Twitter Publisher Handle"
                                                    name="./twitterPublisherHandle"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="twitterPublisherHandle"/>
                                            </twitterPublisherHandle>
                                            <twitterCard
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Twitter Card"
                                                    multiple="{Boolean}false"
                                                    name="./twitterCard"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option0
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Summary Large Image"
                                                            value="summary_large_image"/>
                                                    <option1
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Summary"
                                                            value="summary"/>
                                                    <option2
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="App"
                                                            value="app"/>
                                                    <option3
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}false"
                                                            text="Player"
                                                            value="player"/>
                                                </items>
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="twitterCard"/>
                                            </twitterCard>
                                            <twitterImage
                                                    cq:hideOnEdit="{Boolean}false"
                                                    cq:showOnCreate="{Boolean}true"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    disabled="{Boolean}false"
                                                    fieldLabel="Twitter Image"
                                                    filter="hierarchyNotFile"
                                                    forceSelection="{Boolean}false"
                                                    name="./twitterImage"
                                                    renderReadOnly="{Boolean}true"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam/waters">
                                                <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-msm-lockable="twitterImage"/>
                                            </twitterImage>
                                        </items>
                                    </column>
                                </items>
                            </fixedColumns>
                        </items>
                    </content>
                    <applicationNotes
                        jcr:primaryType="nt:unstructured"
                        path="waters/components/structure/page/applicationnotes/content/items/tabs/items/ApplicationNotes"
                        sling:resourceType="granite/ui/components/foundation/include"
                        sling:orderBefore="socialmedia"/>
                    <errorPages
                        jcr:primaryType="nt:unstructured"
                        path="waters/components/structure/page/errorpages/content"
                        sling:resourceType="granite/ui/components/foundation/include"
                        sling:orderBefore="thumbnail"/>
                    <designReview
                        jcr:primaryType="nt:unstructured"
                        path="waters/components/structure/page/designreview/content"
                        sling:resourceType="granite/ui/components/foundation/include"
                        sling:orderBefore="thumbnail"/>
                    <commerce
                        jcr:primaryType="nt:unstructured"
                        path="waters/components/structure/page/commerce/content/items/tabs/items/Commerce"
                        sling:resourceType="granite/ui/components/foundation/include"
                        sling:orderBefore="thumbnail"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>