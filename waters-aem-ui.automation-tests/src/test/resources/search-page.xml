<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        cq:template="/conf/waters/settings/wcm/templates/search-page"
        sling:resourceType="waters/components/structure/page"
        jcr:title="Search Page Title"
        jcr:description="Search Page Description"
        browserTitle="Search Page Title"
        pageTitle="Search Page Title">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="waters/components/content/layoutcontainer">
            <banner
                jcr:primaryType="nt:unstructured"
                sling:resourceType="waters/components/content/banner">
                <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="waters/components/content/title"/>
                    <searchbar
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="waters/components/content/searchbar"/>
                </par>
            </banner>
        </root>
    </jcr:content>
</jcr:root>